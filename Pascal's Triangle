// this function calculates the value of nCr
long long ncr(int n, int r){
  long long res = 1;

  for(int i=0; i<r; i++){
    res = (res*(n-i))/(i+1);
  }

  return res;
}

// main function
vector<vector<long long int>> printPascal(int n) 
{
  vector<vector<long long>> ans;
  for(int i=1; i<=n; i++){
    vector<long long> v;
    for(int j=1; j<=i; j++){
      v.push_back(ncr(i-1, j-1));
    }
    ans.push_back(v);
  }
